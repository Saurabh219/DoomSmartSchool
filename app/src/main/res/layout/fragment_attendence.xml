<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="notification.push.com.smartschool.Fragment.AttendenceFragment">

    <FrameLayout
        android:layout_width="match_parent"
        android:id="@+id/attendence_frame"
        android:layout_height="wrap_content">

    </FrameLayout>

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/attendence_frame"
        android:layout_marginStart="25dp"
        android:layout_marginTop="10dp"
        android:drawablePadding="10dp"
        android:drawableStart="@drawable/attendent_maker"
        android:gravity="center_vertical"
        android:text="Attendant"
        android:textSize="20sp" />
    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView8"
        android:layout_marginStart="25dp"
        android:layout_marginTop="10dp"
        android:drawablePadding="10dp"
        android:drawableStart="@drawable/not_attendent_maker"
        android:gravity="center_vertical"
        android:text="Not Attendant"
        android:textSize="20sp" />
    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView9"
        android:layout_marginStart="25dp"
        android:layout_marginTop="10dp"
        android:drawablePadding="10dp"
        android:drawableStart="@drawable/half_day_maker"
        android:gravity="center_vertical"
        android:text="Half Day"
        android:textSize="20sp" />
    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView10"
        android:layout_marginStart="25dp"
        android:layout_marginTop="10dp"
        android:drawablePadding="10dp"
        android:drawableStart="@drawable/leaved_maker"
        android:gravity="center_vertical"
        android:text="Leaved School"
        android:textSize="20sp" />
</RelativeLayout>
